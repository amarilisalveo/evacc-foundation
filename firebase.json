{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [],   
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html",
      
        "Target address":"/index.html",
        "Type":"200 (Rewrite)"
      }
    ]
  }
}
